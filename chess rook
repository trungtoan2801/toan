#include<iostream>

using namespace std;
int N;
char M[5][5];
int Answer;

bool check(int row, int col){
	if (M[row][col] == 'X') return false;
	int r = row-1;
	while (r>=0){
		if (M[r][col] == 'X') break;
		if (M[r][col] == '+') return false;
		r--;
	}
	int c = col-1;
	while (c>=0){
		if (M[row][c] == 'X') break;
		if (M[row][c] == '+') return false;
		c--;
	}
	return true;
}

void solve(int pos, int cntRook)
{
	if (pos >= N*N){
		if (cntRook > Answer){
			Answer = cntRook;
		}
		return;
	}
	int row = pos/N;
	int col = pos%N;


	if(check(row,col))
	{
		M[row][col] = '+';
		solve(pos+1, cntRook+1);
		M[row][col] = '.';
	}
	solve(pos+1, cntRook);
}

int main(int argc, char** argv)
{
	int tc;
	int T;
	freopen("input.txt", "r", stdin);
	cin >> T;
	for(tc = 1; tc <= T; ++tc)
	{
		Answer = 0;
		cin>>N;
		for (int i=0;i<N;i++){
			cin>>M[i];
		}
		solve(0, 0);
		cout << "Case #" << tc <<endl<< Answer << endl;
	}
	return 0;
}

// input
1
4
.X..
....
....
....
